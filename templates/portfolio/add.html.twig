{% extends "base.html.twig" %}
{% block header %}
    <h1>Portfolio</h1>
{% endblock %}
{% block body %}
    <h2>Add a project</h2>
    {% for error in errors %}
        <p>{{ error }}</p>
    {% endfor %}
    {% if added %}
        <p>You have successfully added a project to your portfolio!</p>
    {% endif %}
    <div>
    {{ form_start(form) }}
        {{ form_row(form.name) }}
        {{ form_row(form.image) }}
        {{ form_row(form.textDescription) }}
    {{ form_end(form) }}
    </div>
{% endblock %}